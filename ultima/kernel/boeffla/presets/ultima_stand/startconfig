echo '' > /dev/bk_cpu_voltages_profile
echo '1400 1388 1288 1250 1188 1138 1088 1038 988 975 963 938 925 900 900' > /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
echo '1400000' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq
echo '200000' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
