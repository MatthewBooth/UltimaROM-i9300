<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.easylauncher" >

    <home>

        <!--
              appwidget : contact widget, weather widget and calendar widget are fixed
              this is changed only when packageName or className is changed.


        -->
        <!-- Screen 0 -->
        <appwidget
            launcher:className="com.sec.android.widgetapp.easyfavoritescontactswidget.SeniorFavoriteWidgetProviderLarge"
            launcher:iconPosition="0"
            launcher:packageName="com.sec.android.widgetapp.easyfavoritescontactswidget"
            launcher:screen="0" />

        <!-- Screen 1 -->
        <!--
              appshortcut : app shortcut refers to application shortcuts on easy launcher home page
              we can replace the default shortcuts in main screen and favorite apps page by changing the shortcuts information here.


        -->
        <!-- In case of screen 1 position 0 to 5 are occupied by application shortcuts. position above 6 are reserved for application widgets -->

        <appshortcut
            launcher:className="com.sec.android.app.camera.Camera"
            launcher:iconPosition="0"
            launcher:packageName="com.sec.android.app.camera"
            launcher:screen="1" />
        <appshortcut
            launcher:className="com.sec.android.gallery3d.app.Gallery"
            launcher:iconPosition="1"
            launcher:packageName="com.sec.android.gallery3d"
            launcher:screen="1" />
        <appshortcut
             launcher:className="com.android.browser.BrowserActivity"
            launcher:iconPosition="2"
            launcher:packageName="com.android.browser"
            launcher:screen="1" />
        <appshortcut
            launcher:className="com.android.contacts.activities.DialtactsActivity"
            launcher:iconPosition="3"
            launcher:packageName="com.android.contacts"
            launcher:screen="1" />
        <appshortcut
            launcher:className="com.android.contacts.activities.PeopleActivity"
            launcher:iconPosition="4"
            launcher:packageName="com.android.contacts"
            launcher:screen="1" />
        <appshortcut
            launcher:className="com.android.mms.ui.ConversationComposer"
            launcher:iconPosition="5"
            launcher:packageName="com.android.mms"
            launcher:screen="1" />

        <!--
             Screen 1 refers to center screen
            Widgets in center screen: Weather widget (position 6)  Splanner widget( position 7)


        -->
        <appwidget
            launcher:className="com.sec.android.widgetapp.ap.hero.accuweather.easy.widget.WeatherClock"
            launcher:iconPosition="6"
            launcher:packageName="com.sec.android.widgetapp.ap.hero.accuweather"
            launcher:screen="1" />
        <appwidget
            launcher:className="com.sec.android.widgetapp.SPlannerAppWidget.EasyWidget.EasyCalendarAppWidgetProvider"
            launcher:iconPosition="7"
            launcher:packageName="com.sec.android.widgetapp.SPlannerAppWidget"
            launcher:screen="1" />

        <!--
             This easyview is created specifically for launching clock.
		     Clock is not a widget but a layout view defined in EasyLauncher.
		     This easyview will define  which clock app will run. This is required in case any carrier wants to use its own Clock application
		     In case, there is another such view in easy launcher (which is not a widget), we can use this same tag to launch the proper app, but define the proper viewType
		     This view type is used to select the proper package from the database.
		     public static final String VIEW_TYPE[] = {"EASY_CLOCK"}; defined in EasyController.java  will be used as a base to compare against dbvalue
        -->
        <easyview
            launcher:className="com.sec.android.app.clockpackage.ClockPackage"
            launcher:iconPosition="8"
            launcher:packageName="com.sec.android.app.clockpackage"
            launcher:screen="1"
            launcher:viewType="EASY_CLOCK" />

        <!--
       Screen 2 refers to favorite apps screen
           All the default applications shortcuts will be defined here


        -->

        <appshortcut
            launcher:className="com.android.email.activity.Welcome"
            launcher:iconPosition="0"
            launcher:packageName="com.android.email"
            launcher:screen="2" />
        <appshortcut
            launcher:className="com.sec.android.app.clockpackage.ClockPackage"
            launcher:iconPosition="1"
            launcher:packageName="com.sec.android.app.clockpackage"
            launcher:screen="2" />
        <appshortcut
            launcher:className="com.sec.android.app.popupcalculator.Calculator"
            launcher:iconPosition="2"
            launcher:packageName="com.sec.android.app.popupcalculator"
            launcher:screen="2" />
        <appshortcut
            launcher:className="com.sec.android.app.music.MusicActionTabActivity"
            launcher:iconPosition="3"
            launcher:packageName="com.sec.android.app.music"
            launcher:screen="2" />
        <appshortcut
            launcher:className="com.android.calendar.AllInOneActivity"
            launcher:iconPosition="4"
            launcher:packageName="com.android.calendar"
            launcher:screen="2" />
        <appshortcut
            launcher:className="com.infraware.filemanager.FmFileTreeListActivity"
            launcher:iconPosition="5"
            launcher:packageName="com.sec.android.app.snotebook"
            launcher:screen="2" />
        <appshortcut
            launcher:className="com.sec.android.easysettings.EasySettingsHomeActivity"
            launcher:iconPosition="6"
            launcher:packageName="com.sec.android.easysettings"
            launcher:screen="2" />

        <!--
             There are always some applications which are required to be shown in easy mode and not in standard mode and vice-versa.
            So, we have created this tag <easyapps> which contains class name and package name and a flag which says whether the app should be visible.
            If the visibility flag is true, this app will be shown in easy launcher, if false, this app will not be shown in easy launcher


        -->
        <!-- Declare all the Easy apps which are to be shown and laucnhed only from EasyLauncher -->
        <easyapps
            launcher:className="com.sec.android.easysettings.EasySettingsHomeActivity"
            launcher:packageName="com.sec.android.easysettings"
            launcher:visibility="true" />
        <easyapps
            launcher:className="com.samsung.helphub.HelpHubActivity"
            launcher:packageName="com.samsung.helphub"
            launcher:visibility="false" />
    </home>

</favorites>